r.push({\n        name: \"ClinicCheckIn\",\n        params: { clinicId: '5' }\n      });\n    },\n       navigateToCheckOut() {\n      return this.$router.push({\n        name: \"ClinicCheckOut\",\n        params: { clinicId: '5' }\n      });\n    },\n    navigateToMyBookings() {\n      return this.$router.push({ name: \"ManageMyBooking\" });\n    },\n    navigateToAccountSettings() {\n      return this.$router.push({ name: \"AccountView\" });\n    },\n    logout() {\n      var txt;\n  var r = confirm(\"Are you sure want to logout?\");\n  if (r == true) {\n       window.localStorage.setItem('authenticated', false);\n    window.localStorage.setItem('username', false);\n    window.localStorage.setItem('access',false );\n      this.$router.push('/login');\n  } else {\n \n  }\n\n    },\n    navigateToBookAppointment() {\n      return this.$router.push({ name: \"BookAppointment\" });\n    },\n    initChart() {\n      $.plot($(this.$refs.chartContainer), this.getData(), {\n        series: {\n          pie: {\n            innerRadius: 0.8,\n            show: true,\n            fill: 0.5\n          }\n        },\n        colors: [\"#ffc247\", \"#f55d5d\", \"#9964e3\"],\n        legend: {\n          noColumns: 1,\n          container: $(this.$refs.chartLegend),\n          labelBoxBorderColor: \"#ffffff\"\n        }\n      });\n    }\n  },\n  mounted() {\n    \n  \n  },\n  unmounted() {\n  \n  },\n  computed: {\n    calendarAttributes() {\n      return [\n        // Today attribute\n        {\n          contentStyle: {\n            fontWeight: \"700\",\n            fontSize: \".9rem\"\n          },\n          dates: new Date()\n        },\n        // Attributes for todos\n        ...this.todos.map(todo => ({\n          dates: todo.dates,\n          dot: {\n            backgroundColor: todo.color,\n            opacity: todo.isComplete ? 0.3 : 1\n          },\n          popover: {\n            label: todo.description\n          }\n        }))\n      ];\n    }\n  }\n};\n</script